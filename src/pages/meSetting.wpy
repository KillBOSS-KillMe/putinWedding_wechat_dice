          case '1':
            url = `/pages/meSettingPhone`
            break;
          case '2':
            url = `/pages/meSettingAvatarUrl`
            break;
          default:
            return false
        }
        console.log(url)
        this.$navigate(url)
        this.$apply()
      }
    }

    events = {
      
    }

    onLoad() {
    }
    onShow() {
    }
  }
</script>
